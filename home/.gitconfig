[user]
	name = NAMEPLACEHOLDER
	email = EMAILPLACEHOLDER

[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	d = diff
	dp = diff --patience
	padd = add -p

[color]
	ui = auto

[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green

[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold

[color "status"]
	added = green
	changed = yellow
	untracked = cyan
